												 ValueWhenEmpty="0"
												 ValueWhenNonEmpty="*" />
	<Converters:BooleanToGridLengthConverter x:Key="HiddenWhenFalseStarSizedWhenTrue"
																					 ValueWhenFalse="0"
																					 ValueWhenTrue="*"/>
	<Converters:IdStringToIdWithoutHeader x:Key="IdStringToIdWithoutHeader"/>
	<Converters:BundleNameToActiveConverter x:Key="BundleNameToActive"/>
	<Converters:BundleNameToToolTipCoverter x:Key="BundleNameToToolTip"/>
